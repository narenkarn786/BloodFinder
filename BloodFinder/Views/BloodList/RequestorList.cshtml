@model IEnumerable<BloodFinder.Models.BloodList>


<br /><br />
<div class="row">
    <div class="col-lg-1">
        <img src="~/Photos/logo.png" alt="logo" height="90" width="90" />

    </div>
    <div class="col-lg-11">
        <div class="table-responsive-sm">
            <table class="table table-striped">

                <thead class="thead-dark">


                    <tr>
                        <th>Name</th>

                        <th>Address</th>

                        <th>Phone Number</th>
                        <th>E-mail</th>

                        <th>Blood Group</th>
                        <th>Message</th>
                    </tr>
                </thead>

                <tbody>
                    @foreach (var m in Model)
                    {

                        <tr>
                            <td> @m.Name </td>
                            <td>@m.Address</td>
                            <td> @m.PhoneNumber</td>
                            <td>@m.Email</td>
                            <td> @m.Bloodgroup</td>
                            <td>@m.Message</td>
                        </tr>



                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
